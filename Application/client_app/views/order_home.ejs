<!-- Start Menu Area -->
<section class="mt-5 pt-5">
    <div class="row">
        
        <div class="tab menu-list-tab">
            <ul class="tabs justify-content-center">
                <% if (typeof (category) != "undefined" &&  category.length > 0 ) {
                for(let i = 0; i<category.length; i++) {
                %>
                    <li id="cat<%= category[i].cat_id%>" class="catBlock m-3" >
                        <a href="#" class="mb-2" style=" background-image: url('/assets/img/bg-menu.png') ;">
                            <div style="background-image: url('/api/client/1/img/category/<%= category[i].cat_id%>/main.jpg'); height: 80%;  background-size: contain; background-repeat: no-repeat;"></div> 
                            <h4 class="mb-2" style="white-space:nowrap; "><%= category[i].cat_name%></h4>
                        </a>
                    </li>
                <% }
                }%>

            </ul>

            <div class="tab_content scrollbar full_block option_hide " >

                <% if (typeof (category) != "undefined" &&  category.length > 0 ) {
                for(let i = 0; i<category.length; i++) {
                %>

                    <div class="tabs_item pt-10">
                        <div class="row">

                            <% if (typeof (category[i].products) != "undefined" &&  category[i].products.length > 0 ) {
                            for(let j = 0; j<category[i].products.length; j++) {
                            if (category[i].products[j].product_status != 0) {  
                            %>

                                <div class="col-12 col-md-3">
                                    <div class="pizza-shop-item">
                                        <div class="image" style="cursor: pointer;" onclick="order_product_select('<%=order_id%>','<%= category[i].products[j].product_id %>')">
                                            <img src="/api/client/1/img/product/<%= category[i].products[j].product_id %>/main.jpg"
                                                 alt="image" style="height: 350px;">

                                            
                                        </div>

                                        <div class="content">
                                            <h6><%= category[i].products[j].product_name %></h6>
                                           <!--  <h4><%= category[i].products[j].product_desig %></h4> -->
                                            <h4><%= category[i].products[j].product_price %> da</h4>
                                            <div class="pizza-btn" onclick="order_product_select('<%=order_id%>','<%= category[i].products[j].product_id %>')">
                                                <a type="button" class="default-btn text-white">Commander<i class="flaticon-play-button"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            <% }}
                            }%>

                        </div>
                    </div>
                <% }
                }%>
            </div>
        </div>




    </div>

    <div class="offcanvas offcanvas-start" tabindex="-1" id="order_detail_left" aria-labelledby="orderLabel">
        <div class="offcanvas-header">
            <h5 class="offcanvas-title" id="offcanvasScrollingLabel">
                <div class="welcome-content">
                    <span>Votre commande</span>
                </div>
            </h5>
            <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body">

            <div class="col-12" id="order_detail_block">

                <form>
                    <div class="cart-table table-responsive">
                        <!--
                        <table class="table table-bordered">
                            <thead>
                            <tr>
                                <th class="col-2">Produit</th>
                                <th class="col-2">Supplements </th>
                                <th class="col-2">Sauces </th>
                                <th class="col-2">Autres </th>
                                <th class="col-3">Prix </th>
                                <th class="col-1"> # </th>
                            </tr>
                            </thead>
                        </table>
-->
                        <div class="cart-table table-responsive" id="order_table">
                            <table class="table table-bordered">
                                <thead>
                                <tr>
                                    <th>Produit</th>
                                    <!--                                    <th>Supplements </th>-->
                                    <!--                                    <th>Sauces </th>-->
                                    <!--                                    <th>Autres </th>-->
                                    <th>Prix </th>
                                    <th> # </th>
                                </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>

                        <div class="cart-buttons">
                            <div class="row justify-content-between">
                                <div class="col-lg-6 col-sm-6 col-md-6 text-center">
                                    <button type="button" id="btn_order_next" class="btn btn-primary col-lg-10 btn-lg d-flex justify-content-between align-items-center" 
                                    data-bs-dismiss="offcanvas" aria-label="Close">
                                    Suivant
                                    <svg xmlns="http://www.w3.org/2000/svg" width="26" height="26" fill="currentColor" class="bi bi-cart-plus-fill" viewBox="0 0 16 16">
                                     <path d="M.5 1a.5.5 0 0 0 0 1h1.11l.401 1.607 1.498 7.985A.5.5 0 0 0 4 12h1a2 2 0 1 0 0 4 2 2 0 0 0 0-4h7a2 2 0 1 0 0 4 2 2 0 0 0 0-4h1a.5.5 0 0 0 .491-.408l1.5-8A.5.5 0 0 0 14.5 3H2.89l-.405-1.621A.5.5 0 0 0 2 1zM6 14a1 1 0 1 1-2 0 1 1 0 0 1 2 0m7 0a1 1 0 1 1-2 0 1 1 0 0 1 2 0M9 5.5V7h1.5a.5.5 0 0 1 0 1H9v1.5a.5.5 0 0 1-1 0V8H6.5a.5.5 0 0 1 0-1H8V5.5a.5.5 0 0 1 1 0"/>
                                    </svg>
                                  
                                    </button>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6 text-center">
                                    <button id="btn_order_confirm" order_id="<%= order_id %>" class="btn btn-success col-lg-10 btn-lg d-flex justify-content-between align-items-center" 
                                    data-bs-toggle="modal" data-bs-target="#confirmOrderModal">
                                    Confirmer
                                    <svg xmlns="http://www.w3.org/2000/svg" width="26" height="26" fill="currentColor" class="bi bi-check-circle-fill" viewBox="0 0 16 16">
                                    <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0m-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z"/>
                                    </svg>
                                    </button>
                                </div>
                            </div>
                        </div>

                        <div class="cart-totals">
                            <div class="row">
                                <!--<div class="col-6">
                                    <h1 class="text-center" style="font-size: 80px;">
                                        NÂ°<%= order_info[0].order_number %></h1>
                                </div>-->
                                <div class="col-6">
                                    <h2>Total :</h2>
                                    <span>
                                        <h1 id="order_total_price"></h1>
                                    </span>
                                </div>
                            </div>

                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="menu-shape">
        <div class="shape1">
            <img src="/assets/img/image-icon/1.png" alt="image">
        </div>
        <div class="shape2">
            <img src="/assets/img/image-icon/4.png" alt="image">
