
<%- include ("layouts/header.ejs") %>


<h2 class="mb-3">waiting details</h2><% if (typeof (waiting) != "undefined" &&  waiting.length > 0 ) { %>
    <div class="row featurette">
        <div class="col-md-7">
            <h2 class="featurette-heading fw-normal lh-1"><%= waiting[0].waiting_title%> <span class="text-muted">....</span></h2>
            <p class="lead"><%- waiting[0].waiting_content%></p>
        </div>
        <div class="col-md-5">
            <img class="bd-placeholder-img bd-placeholder-img-lg featurette-image img-fluid mx-auto" width="500" height="500" src="/img/default.png"/>
        </div>
    </div>
<% } %>
<%- include ("layouts/footer.ejs") %>


