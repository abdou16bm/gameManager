
<%- include ("layouts/header.ejs") %>


<h2 class="mb-3">user edit </h2><form id="user_edit_form" class="needs-validation" method="post" enctype="multipart/form-data" novalidate>

    <div class="row g-3">

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="user_id_s" class="form-label">user_id_s</label>
            <input type="text" class="form-control" name="user_id_s" id="user_id_s" placeholder="user_id_s" value="<%= user[0].user_id_s%>" required input_add>
            <div class="invalid-feedback">
                Valid user_id_s is required.
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="user_name" class="form-label">user_name</label>
            <input type="text" class="form-control" name="user_name" id="user_name" placeholder="user_name" value="<%= user[0].user_name%>" required input_add>
            <div class="invalid-feedback">
                Valid user_name is required.
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="user_lastname" class="form-label">user_lastname</label>
            <input type="text" class="form-control" name="user_lastname" id="user_lastname" placeholder="user_lastname" value="<%= user[0].user_lastname%>" required input_add>
            <div class="invalid-feedback">
                Valid user_lastname is required.
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="user_email" class="form-label">user_email</label>
            <input type="text" class="form-control" name="user_email" id="user_email" placeholder="user_email" value="<%= user[0].user_email%>" required input_add>
            <div class="invalid-feedback">
                Valid user_email is required.
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="user_phone" class="form-label">user_phone</label>
            <input type="text" class="form-control" name="user_phone" id="user_phone" placeholder="user_phone" value="<%= user[0].user_phone%>" required input_add>
            <div class="invalid-feedback">
                Valid user_phone is required.
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="user_address" class="form-label">user_address</label>
            <input type="text" class="form-control" name="user_address" id="user_address" placeholder="user_address" value="<%= user[0].user_address%>" required input_add>
            <div class="invalid-feedback">
                Valid user_address is required.
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="user_username" class="form-label">user_username</label>
            <input type="text" class="form-control" name="user_username" id="user_username" placeholder="user_username" value="<%= user[0].user_username%>" required input_add>
            <div class="invalid-feedback">
                Valid user_username is required.
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="user_password" class="form-label">user_password</label>
            <input type="text" class="form-control" name="user_password" id="user_password" placeholder="user_password" value="<%= user[0].user_password%>" required input_add>
            <div class="invalid-feedback">
                Valid user_password is required.
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="user_status" class="form-label">user_status</label>
            <input type="text" class="form-control" name="user_status" id="user_status" placeholder="user_status" value="<%= user[0].user_status%>" required input_add>
            <div class="invalid-feedback">
                Valid user_status is required.
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="priv_id" class="form-label">priv_id</label>
            <select type="input_type" class="form-control" name="priv_id" id="priv_id" placeholder="priv_id" required input_add>
                            <% if (typeof foreignDatasObj.priv_id != 'undefined' && foreignDatasObj.priv_id.length > 0 ){
                for(var i = 0;i < foreignDatasObj.priv_id.length;i++) { %>
                <option value="<%= foreignDatasObj.priv_id[i].priv_id %>" <% if (foreignDatasObj.priv_id[i].priv_id == user[0].priv_id) {%> selected <%}%>><%= foreignDatasObj.priv_id[i].table_row %></option>
                <% }} %>          </select>
            <div class="invalid-feedback">
                Valid priv_id is required.
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="main" class="form-label">main</label>
            <input type="file" class="form-control" name="main" id="main" placeholder="main" value="main_value"  input_add>
            <div class="invalid-feedback">
                Valid main is .
            </div>
        </div>

        <div class="col-lg-6 col-sm-6 col-12">
            <label for="file_u" class="form-label">file_u</label>
            <input type="file" class="form-control" name="file_u" id="file_u" placeholder="file_u" value="file_u_value"   multiple="multiple">
            <div class="invalid-feedback">
                Valid file_u is .
            </div>
        </div>

table_inputs    </div>

    <hr class="my-4">

    <button class="w-100 btn btn-primary btn-lg" type="submit">edit</button>
</form>
<%- include ("layouts/footer.ejs") %>


